{
  "shape": {
    "input": {
      "min-height": {
        "base": {
          "value": "calc({sizing.palette.base-unit} * 8)"
        },
        "smaller": {
          "value": "calc({sizing.palette.base-unit} * 6)"
        }
      },
      "border": {
        "width": {
          "value": "{shape.palette.stroke.weight.100.value}"
        },
        "radius": {
          "base": {
            "value": "{shape.palette.stroke.corner-radius.100.value}"
          },
          "smaller": {
            "value": "{shape.palette.stroke.corner-radius.050.value}"
          }
        }
      }
    }
  }
}
