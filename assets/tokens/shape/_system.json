{
  "shape": {
    "system": {
      "stroke": {
        "weight": {
          "100": {
            "value": "1px"
          },
          "200": {
            "value": "2px"
          }
        },
        "corner-radius": {
          "050": {
            "value": "calc({sizing.system.base-unit} / 2)"
          },
          "100": {
            "value": "{sizing.system.base-unit}"
          },
          "200": {
            "value": "calc({sizing.system.base-unit} * 2)"
          },
          "300": {
            "value": "calc({sizing.system.base-unit} * 3)"
          }
        }
      }
    }
  }
}
