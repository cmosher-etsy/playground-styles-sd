{
  "shape": {
    "button": {
      "min-height": {
        "base": {
          "value": "calc({sizing.system.base-unit} * 8)"
        },
        "smaller": {
          "value": "calc({sizing.system.base-unit} * 6)"
        }
      },
      "border": {
        "width": {
          "value": "{shape.system.stroke.weight.200.value}"
        },
        "radius": {
          "base": {
            "value": "calc({sizing.system.base-unit} * 4)"
          },
          "smaller": {
            "value": "calc({sizing.system.base-unit} * 3)"
          }
        }
      }
    }
  }
}
